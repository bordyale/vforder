SELECT VSI.shipment_id, OI.PRODUCT_ID, SI.PRODUCT_ID
FROM `SHIPMENT_ITEM` SI
JOIN `VF_SHIPMENT_ITEM` VSI ON (VSI.SHIPMENT_ID = SI.SHIPMENT_ID AND VSI.SHIPMENT_ITEM_SEQ_ID=SI.SHIPMENT_ITEM_SEQ_ID)
JOIN `ORDER_ITEM` OI ON (VSI.ORDER_ID=OI.ORDER_ID AND VSI.ORDER_ITEM_SEQ_ID=OI.ORDER_ITEM_SEQ_ID)



UPDATE `SHIPMENT_ITEM` SI, `VF_SHIPMENT_ITEM` VSI , `ORDER_ITEM` OI  SET SI.PRODUCT_ID = OI.PRODUCT_ID
WHERE VSI.ORDER_ID=OI.ORDER_ID AND VSI.ORDER_ITEM_SEQ_ID=OI.ORDER_ITEM_SEQ_ID AND VSI.SHIPMENT_ID=SI.SHIPMENT_ID AND VSI.SHIPMENT_ITEM_SEQ_ID=SI.SHIPMENT_ITEM_SEQ_ID