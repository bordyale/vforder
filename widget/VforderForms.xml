    </form>
    
    <form name="ListShippings" type="list" list-name="listIt" paginate-target="findshippings" odd-row-style="alternate-row" default-table-style="basic-table hover-bar">
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="parameters"/>
                <field-map field-name="entityName" value="ShippingHeader"/>
                <field-map field-name="orderBy" value="shippingId"/>              
            </service>
        </actions>
        
        <field name="shippingId" title="${uiLabelMap.ShippingShippingId}" widget-style="buttontext">
            <hyperlink description="${shippingId}" target="EditShipping" also-hidden="false">
                <parameter param-name="shippingId"/>
            </hyperlink>
        </field>
        
        
        
         <!-- <field name="shippingId"><display/></field> -->
        <!-- <field name="shippingId" widget-style="buttontext">
            <hyperlink description="${billingAccountId}" target="EditBillingAccount">
                <parameter param-name="billingAccountId"/>
            </hyperlink>
        </field> -->
        <field name="estimatedShipDate"><display/></field>
	</form>



	<form name="EditShipping" type="single" target="updateShipping" title="" default-map-name="shipping" default-entity-name="ShippingHeader">
        <alt-target use-when="example==null" target="createShipping"/>
        <auto-fields-service service-name="updateShipping"/>
        <field use-when="shipping!=null" name="shippingId" title="${uiLabelMap.ShippingShippingId}"><display/></field>
        <field use-when="shipping==null @and shippingId==null" name="shippingId" title="${uiLabelMap.ShippingShippingId}"><ignored/></field>
        <field use-when="shipping==null @and exampleId!=null" name="shippingId" title="${uiLabelMap.ShippingShippingId}"><display description="${uiLabelMap.CommonCannotBeFound}: [${shippingId}]" also-hidden="false"/></field>
     
        <field name="submitButton" use-when="shipping==null" title="${uiLabelMap.CommonCreate}"><submit button-type="button"/></field>
        <field name="submitButton" use-when="shipping!=null" title="${uiLabelMap.CommonUpdate}"><submit button-type="button"/></field>
    </form>
    
    <form name="EditShippingItems" type="single" target="updateShipping" title="" default-map-name="shipping" default-entity-name="ShippingHeader">
        <alt-target use-when="example==null" target="createShipping"/>
        
        <field name="geoName" title="${uiLabelMap.CommonGeoName}" tooltip="${uiLabelMap.ExampleLookupFields4}" tooltip-style="button-text">
            <lookup target-form-name="LookupOrder" description-field-name="geoId"/>
        </field>
        <!-- This field may be hidden and the name rendered in geoName using default-value if geoId is known when loading the form-->
        <field name="geoId" title="${uiLabelMap.CommonGeoId}"><text/></field><!-- the text field is only used for rendering. In the "real life" you would use the hidden field commented below to pass the data to the request  -->
        <!--field name="geoId"><hidden/></field-->

        
        <sort-order>
            <field-group>
                <sort-field name="geoName"/>
                <sort-field name="geoId"/>
            </field-group>
        </sort-order>
        
    </form>
    
    
     <form name="ListShippingItems" type="list" list-name="listIt" paginate-target="findshippings" odd-row-style="alternate-row" default-table-style="basic-table hover-bar">
        <actions>
            <service service-name="performFind" result-map="result" result-map-list="listIt">
                <field-map field-name="inputFields" from-field="parameters"/>
                <field-map field-name="entityName" value="ShippingItem"/>
                <field-map field-name="orderBy" value="shippingItemSeqId"/>              
            </service>
        </actions>
        
       <field name="shippingId"><display/></field>
        
        
        
         <!-- <field name="shippingId"><display/></field> -->
        <!-- <field name="shippingId" widget-style="buttontext">
            <hyperlink description="${billingAccountId}" target="EditBillingAccount">
                <parameter param-name="billingAccountId"/>
            </hyperlink>
        </field> -->
        
	</form>
	
	
	<form name="AddShippingItemOrderItem" type="multi" use-row-submit="true" list-name="orderItems" title="" target="UpdateShippingItems"
        odd-row-style="alternate-row" default-table-style="basic-table hover-bar" paginate-target="AddShippingItemOrderItem">
        <!-- <row-actions>
            <entity-one entity-name="OrderItem" value-field="product" auto-field-map="true"/>
        </row-actions> -->
        <field name="shippingId"><hidden value="${shippingId}"/></field>
       <field name="orderId"><hidden value="${orderId}"/></field>
       <field name="orderItemSeqId"><hidden value="${orderItemSeqId}"/></field>
        <field name="orderId"  title="${uiLabelMap.OrderOrderId}" widget-style="buttontext">
            <hyperlink description="${orderId}" target="/catalog/control/EditProductInventoryItems" target-type="inter-app">
                <parameter param-name="orderId"/>
            </hyperlink>
        </field>
        <field name="productId"><display/></field>
        <field name="quantity" title="${uiLabelMap.OrderItemQuantity}"><display/></field>
        <field name="quantityShipped"><display/></field>
        <field name="quantityShippable"><display/></field>
        <field name="quantityToShip" title="${uiLabelMap.OrderQuantity}">
            <text size="5" maxlength="10"/>
        </field>
        <field name="submitButton" title="${uiLabelMap.OrderAddToOrder}" widget-style="smallSubmit">
            <submit/>
        </field>
    </form>
    
    
    

</forms>