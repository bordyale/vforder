	<form name="ListShippings" type="list" list-name="listIt"
		paginate-target="findshippings" odd-row-style="alternate-row"
		default-table-style="basic-table hover-bar" header-row-style="header-row-2"
		override-list-size="true">
		<actions>
			<service service-name="performFind" result-map="result"
				result-map-list="listIt">
				<field-map field-name="inputFields" from-field="parameters" />
				<field-map field-name="entityName" value="Shipment" />
				<field-map field-name="orderBy" from-field="sortField" />
			</service>
		</actions>

		<field name="shipmentId" title="${uiLabelMap.ShippingshipmentId}"
			widget-style="buttontext" sort-field="true">
			<hyperlink description="${shipmentId}" target="EditShipping"
				also-hidden="false">
				<parameter param-name="shipmentId" />
			</hyperlink>
		</field>



		<!-- <field name="shipmentId"><display/></field> -->
		<!-- <field name="shipmentId" widget-style="buttontext"> <hyperlink description="${billingAccountId}" 
			target="EditBillingAccount"> <parameter param-name="billingAccountId"/> </hyperlink> 
			</field> -->
		<field name="estimatedShipDate" sort-field="true">
			<display
				description="${groovy:org.apache.ofbiz.base.util.UtilDateTime.toDateString(estimatedShipDate, &quot;yyyy.MM.dd.&quot;);}" />
		</field>


	</form>


	<form name="EditShipping" type="single" target="updateShipment"
		title="" default-map-name="shipping" default-entity-name="Shipment">
		<alt-target use-when="shipping==null" target="createShipment" />
		<!-- <auto-fields-service service-name="updateShipment"/> -->
		<field use-when="shipping!=null" name="shipmentId"
			title="${uiLabelMap.ShippingshipmentId}">
			<display />
		</field>
		<field use-when="shipping==null @and shipmentId==null" name="shipmentId"
			title="${uiLabelMap.ShippingshipmentId}">
			<ignored />
		</field>
		<field use-when="shipping==null @and shipmentId!=null" name="shipmentId"
			title="${uiLabelMap.ShippingshipmentId}">
			<display description="${uiLabelMap.CommonCannotBeFound}: [${shipmentId}]"
				also-hidden="false" />
		</field>

		<field use-when="shipping==null" name="shipmentTypeId">
			<hidden value="SALES_SHIPMENT" />
		</field>
		<field name="estimatedShipDate">
			<date-time />
		</field>

		<field name="submitButton" use-when="shipping==null" title="${uiLabelMap.CommonCreate}">
			<submit button-type="button" />
		</field>
		<field name="submitButton" use-when="shipping!=null" title="${uiLabelMap.CommonUpdate}">
			<submit button-type="button" />
		</field>
		<!-- <field name="deleteLink" use-when="shipping!=null" title=" " widget-style="buttontext"> 
			<hyperlink description="${uiLabelMap.CommonDelete}" target="deleteShipping" 
			also-hidden="false" confirmation-message="${uiLabelMap.CommonConfirmDelete}" 
			image-location="${iconsLocation}/delete.png"> <parameter param-name="shipmentId"/> 
			</hyperlink> </field> -->


	</form>

