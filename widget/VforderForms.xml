	<form name="ListShippingItems" type="multi" list-name="listIt"
		target="UpdatePallet" paginate-target="findshippings" odd-row-style="alternate-row"
		default-table-style="basic-table hover-bar">
		<!-- <actions> <service service-name="performFind" result-map="result" 
			result-map-list="listIt"> <field-map field-name="inputFields" from-field="findParams"/> 
			<field-map field-name="entityName" value="ShippingItem"/> <field-map field-name="orderBy" 
			value="shippingItemSeqId"/> </service> </actions> -->

		<field name="shipmentId">
			<hidden />
		</field>

		<field name="shipmentItemSeqId">
			<display />
		</field>
		<field name="orderId">
			<display />
		</field>
		<!-- <field name="orderItemSeqId"><display/></field> -->
		<field name="productId">
			<display />
		</field>
		<field name="productName">
			<display />
		</field>

		<field name="quantity">
			<display />
		</field>
		<field name="boxNumber">
			<display />
		</field>
		<field name="pallet" title="${uiLabelMap.OrderPallettization}">
			<text size="5" maxlength="10" />
		</field>
		<field name="isBoxOrPallet" title="${uiLabelMap.OrderIsBoxOrPallet}">
			<text size="5" maxlength="10" />
		</field>
		<field name="piecesPerBox" title="${uiLabelMap.OrderPiecesPerBox}">
			<text size="5" maxlength="10" />
		</field>


		<field name="deleteLink" title=" " widget-style="buttontext"
			use-when="${groovy: !'SHIPMENT_SHIPPED'.equals(shipping.statusId)}">
			<hyperlink description="${uiLabelMap.CommonDelete}" target="deleteShippingItem"
				also-hidden="false" confirmation-message="${uiLabelMap.CommonConfirmDelete}"
				image-location="${iconsLocation}/delete.png">
				<parameter param-name="shipmentId" />
				<parameter param-name="orderId" />
				<parameter param-name="orderItemSeqId" />
				<parameter param-name="shipmentItemSeqId" />
				<parameter param-name="quantityToShip" />
			</hyperlink>
		</field>
		<field name="deleteLink" title=" " widget-style="buttontext disabled"
			use-when="${groovy: 'SHIPMENT_SHIPPED'.equals(shipping.statusId)}">
			<display description="${uiLabelMap.DeleteDisabled}"></display>
		</field>



		<field name="submitButton" title="${uiLabelMap.ShippingUpdatePallet}"
			widget-style="smallSubmit">
			<submit />
		</field>

		<!-- <field name="shipmentId"><display/></field> -->
		<!-- <field name="shipmentId" widget-style="buttontext"> <hyperlink description="${billingAccountId}" 
			target="EditBillingAccount"> <parameter param-name="billingAccountId"/> </hyperlink> 
			</field> -->

	</form>


	<form name="AddShippingItemOrderItem" type="multi" use-row-submit="true"
		list-name="orderItems" title="" target="UpdateShippingItems"
		odd-row-style="alternate-row" default-table-style="basic-table hover-bar"
		paginate-target="AddShippingItemOrderItem">
		<!-- <row-actions> <entity-one entity-name="OrderItem" value-field="product" 
			auto-field-map="true"/> </row-actions> -->
		<field name="shipmentId">
			<hidden value="${shipmentId}" />
		</field>
		<field name="orderId">
			<hidden value="${orderId}" />
		</field>
		<field name="orderItemSeqId">
			<hidden value="${orderItemSeqId}" />
		</field>
		<!-- <field name="orderId" title="${uiLabelMap.OrderOrderId}" widget-style="buttontext"> 
			<hyperlink description="${orderId}" target="/catalog/control/EditProductInventoryItems" 
			target-type="inter-app"> <parameter param-name="orderId"/> </hyperlink> </field> -->
		<field name="productId">
			<display />
		</field>
		<field name="productName">
			<display />
		</field>
		<field name="quantity" title="${uiLabelMap.OrderItemQuantity}">
			<display />
		</field>
		<field name="quantityShipped">
			<display />
		</field>
		<field name="quantityShippable">
			<display />
		</field>
		<field name="quantityToShip" title="${uiLabelMap.OrderQuantity}">
			<text size="5" maxlength="10" />
		</field>
		<field name="submitButton" title="${uiLabelMap.OrderAddToOrder}"
			widget-style="smallSubmit">
			<submit />
		</field>

	</form>

	<form name="LookupBulkAddProducts" type="multi" use-row-submit="true"
		list-name="productList" title="" target="BulkAddProducts"
		odd-row-style="alternate-row" default-table-style="basic-table hover-bar"
		paginate-target="LookupBulkAddProducts">
		<row-actions>
			<entity-one entity-name="Product" value-field="product"
				auto-field-map="true" />
			<set field="" />
			<set field="hasAmount" value="${groovy:product.getString('requireAmount')?:'N'}" />
		</row-actions>
		<field name="productId" title="${uiLabelMap.ProductProductId}"
			widget-style="buttontext">
			<hyperlink description="${productId}"
				target="/catalog/control/EditProductInventoryItems" target-type="inter-app">
				<parameter param-name="productId" />
			</hyperlink>
		</field>
		<!-- <field name="brandName" title="${uiLabelMap.ProductBrandName}"><display/></field> -->
		<field name="internalName">
			<display />
		</field>
		<field name="quantity" title="${uiLabelMap.OrderQuantity}">
			<text size="5" maxlength="10" />
		</field>
		<field use-when="${groovy: 'Y'.equals(context.hasAmount)}" name="amount"
			title="${uiLabelMap.OrderAmount}">
			<text size="5" maxlength="10" />
		</field>
		<field use-when="${groovy: 'N'.equals(context.hasAmount)}" name="amount"
			title="${uiLabelMap.OrderAmount}" widget-style="disabled">
			<text size="5" maxlength="10" disabled="true" />
		</field>
		<!-- <field name="itemDesiredDeliveryDate" title="${uiLabelMap.OrderDesiredDeliveryDate}"> 
			<date-time/> </field> -->
		<field name="submitButton" title="${uiLabelMap.OrderAddToOrder}"
			widget-style="smallSubmit">
			<submit />
		</field>
	</form>
	<form name="LookupBulkAddSupplierProducts" type="multi"
		use-row-submit="true" list-name="productList" title="" target="BulkAddProducts"
		odd-row-style="alternate-row" default-table-style="basic-table hover-bar"
