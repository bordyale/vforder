								<parameter param-name="shipmentId" />
							</link>
						</container>

						<!-- <decorator-section name="search-options"> <include-form name="EditShippingItems" 
							location="component://vforder/widget/VforderForms.xml"/> </decorator-section> -->

						<include-form name="ListShippingItems"
							location="component://vforder/widget/VforderForms.xml" />

